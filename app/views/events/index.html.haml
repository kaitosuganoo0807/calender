%body#particles-js
  = render "header"
  %h1 calender.space
  - @events.each do |event|
    %tr
    .botton
      %td= link_to 'Show', event,class:"show"
      %td= link_to 'Edit', edit_event_path(event),class:"edit"
      %td= link_to 'Destroy', event,class:"destroy", method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Event', new_event_path,class:"new-event"
= link_to 'Home Calendar', root_path,class:"home"

%h2 calender
#calendar

= render "footer"